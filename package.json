{
  "dependencies": {
    "esbuild": "^0.18.11",
    "jquery": "^3.7.0",
    "jquery-ujs": "^1.2.3",
    "sass": "^1.63.6",
    "selectize": "^0.12.6"
  },
  "scripts": {
    "build": "esbuild app/javascript/administrate/application.js --bundle --sourcemap --minify --outdir=app/assets/builds/administrate --public-path=/assets/administrate",
    "build:css": "yarn build:css:application && yarn build:css:docs",
    "build:css:application": "sass ./app/assets/stylesheets/administrate/application.sass.scss:./app/assets/builds/administrate/application.css --source-map --style=compressed --load-path=node_modules",
    "build:css:docs": "sass ./app/assets/stylesheets/administrate/docs.scss:./app/assets/builds/administrate/docs.css --source-map --style=compressed --load-path=node_modules"
  }
}
